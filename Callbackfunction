function each(arr, callback) {
  
return callback(arr)
  
 } 
 

function getLower(elem) {

return elem.map((item ) => item[0].toLowerCase() + item.slice(1))

}



let str = ['Alena' , 'Katya' , 'Masha'];
console.log(each(str,getLower ))
